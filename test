export async function getStaticProps() {
    try {
      const response =  await getVehicleList();
      console.log(response);
      if (!response) {
        throw { message: "getStaticProps: Vehicles not found" };
      }
     
      return {
        props: { vehicles:response.data },

      };
    } catch (error) {
      console.log(error);
      return {
        props: {
          vehicles: [],
        },
      };
    }
}

